# DeepSeek API Configuration
# Get your API key from: https://platform.deepseek.com/
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Application Settings
APP_TITLE="AI Data Analysis Agent"
APP_DEBUG=false
APP_LOG_LEVEL=INFO

# DuckDB Configuration
DUCKDB_MEMORY_LIMIT=2GB
DUCKDB_THREADS=4
DUCKDB_TEMP_DIRECTORY=./temp

# File Upload Settings
UPLOAD_MAX_SIZE_MB=100
UPLOAD_ALLOWED_EXTENSIONS=.csv,.xlsx,.xls,.parquet,.json
UPLOAD_TEMP_DIR=./data_uploads

# Query Settings
DEFAULT_ROW_LIMIT=200
MAX_ROW_LIMIT=10000
QUERY_TIMEOUT_SECONDS=30

# Visualization Settings
ENABLE_ADVANCED_VISUALIZATIONS=true
CHART_THEME=streamlit
MAX_CHART_POINTS=1000

# Security Settings
ALLOW_DROP_TABLE=false
ALLOW_DELETE_OPERATIONS=false
ALLOW_FILE_DOWNLOAD=true

# Optional: Proxy configuration (if needed)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=http://proxy.example.com:8080

# Optional: Custom model configuration
# DEEPSEEK_MODEL=deepseek-chat
# DEEPSEEK_TEMPERATURE=0.1
# DEEPSEEK_MAX_TOKENS=1000