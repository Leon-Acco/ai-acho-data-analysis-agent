# AI 数据分析 Agent - 多模型配置示例
# ==========================================
# 各模型 API Key 获取地址:
# - DeepSeek: https://platform.deepseek.com/
# - 智谱(ChatGLM): https://open.bigmodel.cn/dev/howuse/introduction
# - 豆包(Doubao): https://www.volcengine.com/product/doubao
# - 千问(Qwen): https://dashscope.console.aliyun.com/
# ==========================================

# DeepSeek (默认)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# 智谱(ChatGLM)
ZHIPU_API_KEY=your_zhipu_api_key_here

# 豆包(Doubao)
DOUBAO_API_KEY=your_doubao_api_key_here

# 千问(Qwen)
QWEN_API_KEY=your_qwen_api_key_here

# OpenAI (可选)
OPENAI_API_KEY=your_openai_api_key_here

# ==========================================
# 应用设置
# ==========================================
APP_TITLE="AI 数据分析 Agent"
APP_DESCRIPTION="支持多模型的数据分析工具"
APP_DEBUG=false
APP_LOG_LEVEL=INFO

# ==========================================
# DuckDB 配置
# ==========================================
DUCKDB_MEMORY_LIMIT=2GB
DUCKDB_THREADS=4
DUCKDB_TEMP_DIRECTORY=./temp

# ==========================================
# 文件上传设置
# ==========================================
UPLOAD_MAX_SIZE_MB=100
UPLOAD_ALLOWED_EXTENSIONS=.csv,.xlsx,.xls,.parquet,.json
UPLOAD_TEMP_DIR=./data_uploads

# ==========================================
# 查询设置
# ==========================================
DEFAULT_ROW_LIMIT=200
MAX_ROW_LIMIT=10000
QUERY_TIMEOUT_SECONDS=30

# ==========================================
# 可视化设置
# ==========================================
ENABLE_ADVANCED_VISUALIZATIONS=true
CHART_THEME=streamlit
MAX_CHART_POINTS=1000

# ==========================================
# 安全设置
# ==========================================
ALLOW_DROP_TABLE=false
ALLOW_DELETE_OPERATIONS=false
ALLOW_FILE_DOWNLOAD=true

# ==========================================
# 默认模型设置
# ==========================================
# 可选模型列表:
# - deepseek-chat (DeepSeek 默认)
# - deepseek-reasoner (DeepSeek 推理模型)
# - glm-4 (智谱 GLM-4)
# - glm-4-plus (智谱 GLM-4-Plus)
# - glm-3-turbo (智谱 GLM-3-Turbo)
# - doubao-pro-32k (豆包 32K)
# - doubao-pro-128k (豆包 128K)
# - qwen-turbo (千问 Turbo)
# - qwen-plus (千问 Plus)
# - qwen-max (千问 Max)
# - gpt-4o (OpenAI GPT-4o)
# - gpt-4o-mini (OpenAI GPT-4o Mini)
DEFAULT_MODEL=deepseek-chat

# ==========================================
# 模型参数 (可选)
# ==========================================
MODEL_TEMPERATURE=0.1
MODEL_MAX_TOKENS=1000
